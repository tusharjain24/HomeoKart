<body>
    <div class="bod-cont">
        <div class="nav-cont">
            <nav class="navbar fixed-top navbar-expand-lg navbar-light " style="background-color: #e3f2fd;">
                <div class="container-fluid">
                    <div class="nav-brand-container">
                        <a class="navbar-brand" href="/">HomeoKart</a>
                        <p class="logo">
                            Heal with Homeopathy!</p>
                    </div>

                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                        data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="items navbar-nav ml-auto mb-2 mb-lg-0">
                            <!-- me-auto -->
                            <!-- <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="#">Home</a>
          </li> -->
                            <div class="flex-around">
                                <li class="nav-item item1">
                                    <a class="nav-link " href="/categories">Medicines</a>
                                </li>
                                <li class="nav-item item1">
                                    <a class="nav-link" href="/product">Consultation</a>
                                </li>
                                <li class="nav-item item1">
                                    <a class="nav-link" href="#">Blog</a>
                                </li>
                                <li class="nav-item item1">
                                    <a class="nav-link" href="/uploadData">Admin</a>
                                </li>
                            </div>
                            <div class="flex-around">
                                <li class="nav-item item2">
                                    <form class="d-flex">
                                        <input class="form-control me-2 search-bar" type="search"
                                            placeholder="Search medicine and healthcare products" aria-label="Search">
                                        <button class="btn btn-primary" type="submit">Search</button>
                                    </form>
                                </li>

                                <li class="nav-item item3">
                                    <a class="nav-link" href="#"><i class="fas fa-user fa-1x"></i> Login</a>

                                </li>
                                <li class="nav-item item3">
                                    <a class="nav-link" href="/cart"><i class="fa fa-shopping-cart fa-1x"></i> Cart</a>
                                </li>
                                </li>
                            </div>
                        </ul>

                    </div>
                </div>
            </nav>
        </div>

        <div class="prd-body">
            <div class="prd-img">
                <div class="slider-for">
                    <div>
                        <img src="/images/7.jpg" loading="lazy" class="imag">
                    </div>
                    <div>
                        <img src="/images/8.jpg" loading="lazy" class="imag">
                    </div>
                    <div>
                        <img src="/images/9.jpg" loading="lazy" class="imag">
                    </div>
                </div>
                <div class="slider-nav">
                    <div>
                        <img src="/images/7.jpg" loading="lazy" class="imag-nav">
                    </div>
                    <div>
                        <img src="/images/8.jpg" loading="lazy" class="imag-nav">
                    </div>
                    <div>
                        <img src="/images/9.jpg" loading="lazy" class="imag-nav">
                    </div>
                </div>
            </div>
            <div class="prd-content">
                <div class="prd-title">
                    <div class="hdr"><%= product.name%></div>
                    <div class="categ">
                        <% console.log(product.tag) %>

                    </div>
                    <hr>
                    <div class="price">
                        <div class="adj">Special Price</div>
                        <div class="prc">
                            <div class="amt">₹<%=product.price%></div>
                            <div class="dsc"> Get 76% off</div>
                        </div>
                    </div>
                    <div class="rate">
                        <div class="stars"><%=product.ratings %> <i class="fas fa-star"></i></div>
                        <div class="norat">7,614 ratings</div>
                        <div class="assure"><span>H</span>Assured</div>
                    </div>

                    <div class="info">
                        <div class="inf"><i class="fas fa-arrow-right"></i>This product cannot be returned for a refund
                            or exchange.</div>
                        <div class="inf"><i class="fas fa-arrow-right"></i>Mkt: Ramaraju Surgical Cotton Mills Ltd</div>
                        <div class="inf"><i class="fas fa-arrow-right"></i>Country of Origin: India</div>
                        <div class="inf"><i class="fas fa-arrow-right"></i>Delivery charges if applicable will be
                            applied at checkout</div>
                    </div>

                    <div class="buy-section">
                        <button type="button" class="btn btn-primary bton1"><i class="fas fa-bolt"></i> Buy Now</button>
                        <a href="/cart/<%= product.id%>"><button type="button" class="btn btn-primary bton2"><i
                                    class="fas fa-shopping-cart"></i> Add to
                                Cart</button></a>
                    </div>

                    <div class="services">
                        <div class="src">
                            <img src="/images/10.png" loading="lazy" />
                            <div class="one ">Cash on Delivery</div>
                        </div>
                        <div class="src">
                            <img src="/images/11.png" loading="lazy" />
                            <div class="two">7 days Replacement policy</div>
                        </div>
                        <div class="src">
                            <img src="/images/12.png" loading="lazy" />
                            <div class="three">HomeoKart delivered</div>
                        </div>
                        <div class="src">
                            <img src="/images/13.png" loading="lazy" />
                            <div class="four">12 Month Warranty</div>
                        </div>
                    </div>
                </div>


                <!-- Copyright -->
                <div class="footer-copyright text-center py-3">© 2022 Copyright:
                    <a href="#">HomeoKart</a>
                    <p>The information provided on this site is intended for your general information only and not a
                        substitute
                        for
                        professional medical advice or treatment of medical conditions. We recommend you seek
                        consultation from
                        a
                        qualified Homeopath regarding any medical condition.</p>
                </div>
                <!-- Copyright -->
            </div>
            </footer>
            <!-- Footer -->

